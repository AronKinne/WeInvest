<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- Colors -->
    <Color x:Key="PrimaryColor1" R="46" G="139" B="87" A="255"/> <!-- green -->
    <Color x:Key="SecondaryColor" R="245" G="245" B="245" A="255"/> <!--gray-->

    <!-- Brushes -->
    <SolidColorBrush x:Key="PrimaryBrush1" Color="{StaticResource PrimaryColor1}"/>
    <SolidColorBrush x:Key="PrimaryBrush2" Color="DarkGreen"/>
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource SecondaryColor}"/>

    <!-- Fonts -->
    <FontFamily x:Key="Font">pack://application:,,,/WeInvest;Component/Fonts/#Dosis</FontFamily>

</ResourceDictionary>